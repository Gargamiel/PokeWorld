<Defs>

  <RecipeDef Name="PW_ResurrectPokemonFossil" Abstract="True">
    <label>resurrect fossilized Pokémon</label>
    <description>Bring an ancient Pokémon back to life with DNA from its Fossil.</description>
    <jobString>Bringing Pokémon back to life.</jobString>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <workAmount>12000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <recipeUsers>
      <li>PW_FossilResurrector</li>
    </recipeUsers>
    <researchPrerequisites>
      <li>PW_FossilResurrectorResearch</li>
    </researchPrerequisites>
    <skillRequirements>
      <Intellectual>6</Intellectual>
    </skillRequirements>
  </RecipeDef>

  <RecipeDef ParentName="PW_ResurrectPokemonFossil">
    <defName>PW_ResurrectHelixFossil</defName>
    <label>resurrect Helix Fossil</label>
    <unfinishedThingDef>PW_UnfinishedHelixFossil</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>PW_HelixFossil</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>PW_HelixFossil</li>
      </thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <thingDefs>
        <li>PW_HelixFossil</li>
      </thingDefs>
    </defaultIngredientFilter>
    <products>
      <PW_Omanyte>1</PW_Omanyte>
    </products>
  </RecipeDef>

